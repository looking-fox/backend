{
  "extends": ["standard", "prettier"],
  "plugins": ["import", "jest"],
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "camelcase": 2,
    "no-throw-literal": 0,
    "handle-callback-err": 0,
    "standard/no-callback-literal": [0]
  }
}
